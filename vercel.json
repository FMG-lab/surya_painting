{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "public": false,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs22.x",
      "memory": 512,
      "maxDuration": 10
    }
  }
}
